{
  "name": "tree-sitter-fsharp",
  "version": "0.0.1",
  "description": "",
  "repository": "https://github.com/tree-sitter/tree-sitter-fsharp",
  "license": "MIT",
  "author": "Nikolaj Sidorenco",
  "main": "bindings/node",
  "types": "bindings/node",
  "files": [
    "binding.gyp",
    "bindings/node/*",
    "grammars/fsharp/src/**",
    "queries/*",
    "*.wasm"
  ],
  "devDependencies": {
    "eslint": "^9.8.0",
    "eslint-config-google": "^0.14.0",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.22.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "build": "npm run build-fsharp && npm run build-interface",
    "build-fsharp": "cd grammars/fsharp && tree-sitter generate --no-bindings",
    "build-interface": "cd grammars/interface && tree-sitter generate --no-bindings",
    "test": "npm run test-fsharp && npm run test-interface && npm run test-highlighting",
    "test-fsharp": "cd grammars/fsharp && tree-sitter test",
    "test-interface": "cd grammars/interface && tree-sitter test",
    "test-highlighting": "tree-sitter test",
    "lint": "eslint grammar.js",
    "parse": "tree-sitter parse",
    "debug": "tree-sitter parse -d",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "source.fsharp",
      "path": "grammars/fsharp",
      "file-types": [
        "fs",
        "fsx"
      ],
      "injection-regex": "^(fsharp|fsx?)$",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "tags": "queries/tags.scm"
    },
    {
      "scope": "source.fsharp.interface",
      "path": "grammars/interface",
      "file-types": [
        "fsi"
      ],
      "injection-regex": "^(fsharp_interface|fsi)$",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "dependencies": {
    "node-addon-api": "^8.1.0",
    "node-gyp-build": "^4.8.1"
  }
}
